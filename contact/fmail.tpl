    <script>
    $(document).ready(function() {
        var mode = location.search;
        if (mode === "?mode=confirm" || mode === "?mode=error" || mode === "?mode=back" || mode === "?mode=send" || mode === "?mode=thanks" || mode === "?mode=tab1" || mode === "?mode=tab2" || mode === "?mode=tab3") {

            if ($('#fmail_form').length) {
                var _mailFormPos = $('#fmail_form').offset().top;
                _mailFormPos = parseInt(_mailFormPos);

                var offsetPC = 240;
                var offsetMid = 160;
                var offsetSP = 90;
                if ($(window).width() > 750 && $(window).width() < 1903) {
                    $('html,body').animate({ scrollTop: _mailFormPos - offsetMid }, 400);
                }

                if ($(window).width() >= 1903) {
                    $('html,body').animate({ scrollTop: _mailFormPos - offsetPC }, 400);
                }

                if ($(window).width() <= 750) {
                    $('html,body').animate({ scrollTop: _mailFormPos - offsetSP }, 400);
                }
            }
        }



        $(window).on('load', function() {

            $("#d_en1655367791, #d_en1655368038, #d_en1655367978, #d_en1663991936, #d_en1663991268").attr("colspan", "2");

            function tab1_click() {
                $('#fmail table.tbl-ctr01').show();
                $('#fmail table.tbl-ctr02').hide();
                $('#fmail table.tbl-ctr03').hide();
            }

            function tab2_click() {
                $('#fmail table.tbl-ctr01').hide();
                $('#fmail table.tbl-ctr03').hide();
                $('#fmail table.tbl-ctr02').show();
            }

            function tab3_click() {
                $('#fmail table.tbl-ctr01').hide();
                $('#fmail table.tbl-ctr02').hide();
                $('#fmail table.tbl-ctr03').show();
            }
            var ss = sessionStorage.getItem('select');
            // console.log(ss);
            tab1_click();
            $('#d_en1655367791 label').removeClass('fmail_label_enabled');

            function action_btn_fmail(params) {
                if (params == 'tab3') {
                    $('#ttl_contact').text('求人応募フォーム');
                    $('#en1655367791_03_label').trigger('click').removeClass('fmail_label_disabled').addClass('fmail_label_enabled');
                    tab3_click();
                } else if (params == 'tab2') {
                    $('#ttl_contact').text('資料請求フォーム');
                    $('#en1655367791_02_label').trigger('click').removeClass('fmail_label_disabled').addClass('fmail_label_enabled');
                    tab2_click();
                } else {
                    $('#ttl_contact').text('お問い合わせ・見積もり依頼フォーム');
                    $('#en1655367791_01_label').trigger('click').removeClass('fmail_label_disabled').addClass('fmail_label_enabled');
                    tab1_click();
                }
                $('#en1655367791_01_label').click(function() {
                    tab1_click();
                    sessionStorage.setItem('select', 'tab1');
                    $("#en1240790903").val(''); // tab 2 click               
                    $('#fmail table.tbl-ctr02 input,#fmail table.tbl-ctr02 textarea, #fmail table.tbl-ctr02 select').not(".postcode").val('');
                });
                $('#en1655367791_02_label').click(function() {
                    tab2_click();
                    sessionStorage.setItem('select', 'tab2');
                    $("#en1655372639").val(''); // tab 1click
                    $('#fmail table.tbl-ctr01 input,#fmail table.tbl-ctr01 textarea, #fmail table.tbl-ctr01 select').not(".postcode").val('');
                    $('#en1240790903').val('');
                });
                $('#en1655367791_03_label').click(function() {
                    tab3_click();
                    sessionStorage.setItem('select', 'tab3');
                });
            }
            action_btn_fmail(ss);

            $('.udr_btn_anchor li a').click(function() {
                var data = $(this).attr('data-fml');

                if (!data == '') {
                    $('#d_en1655367791 label').removeClass('fmail_label_enabled');
                    action_btn_fmail(data);
                }

            });

            $('.fx-btn').click(function() {
                $('.udr_btn_anchor li a').trigger('click');
            });


            if (mode === "?mode=tab1" || mode === "?mode=tab2" || mode === "?mode=tab3") {
                $('#d_en1655367791 label').removeClass('fmail_label_enabled');
                var dataLocation = location.search;
                let getText = dataLocation.slice(6);
                action_btn_fmail(getText);
            }
        });
    });
    </script>
